{
  "name": "boostrap-5-unibit-theme",
  "version": "1.0.0",
  "description": "Default Boostrap 5 starter theme for Unibit",
  "main": "public/index.html",
  "scripts": {
    "build": "unibit build",
    "dev": "unibit develop",
    "start": "unibit develop",
    "serve": "http-server ./public",
    "js-watch": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --watch --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "js-dev": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "js-prod": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NicolasRoehm/boostrap-5-unibit-theme"
  },
  "author": "Stackbit",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NicolasRoehm/boostrap-5-unibit-theme/issues"
  },
  "homepage": "https://docs.stackbit.com/unibit/",
  "dependencies": {
    "@popperjs/core": "^2.6.0",
    "bootstrap": "^5.0.0-beta1",
    "laravel-mix": "^5.0.5"
  },
  "devDependencies": {
    "@stackbit/unibit": "~0.1.31",
    "@types/bootstrap": "^5.0.5",
    "@types/node": "^14.11.1",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "expose-loader": "^0.7.5",
    "nodemon": "^1.19.1",
    "resolve-url-loader": "^3.1.0",
    "sass-loader": "^8.0.2",
    "ts-loader": "^6.2.2",
    "typescript": "^3.9.7"
  }
}
